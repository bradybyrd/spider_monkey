/* MAIN DISPLAY AREA (#mainDisplay) */
#mainDisplay {
    font-family: 'Arial', 'Tahoma', Helvetica, Arial, sans-serif;
}
/* above the content (#aboveContent) */
#aboveContent p.text {
    margin: 0;
    padding: 5px 0 10px;
}
/* content area (#content) */
/* main heading for content area */
#mainDisplay h2 {
    display: block;
    color: #004376;
    padding: 5px 5px 5px 15px;
    border-bottom: 1px solid #BBC4C9;
    margin: 0 0 0 -15px;
    text-align: left;
    font-size: 1.27em; /* 14pt */
}
/* icons in main heading for content area */
#mainDisplay h2 img {
    float: right;
    margin-top:-20px;
    margin-right: 10px;
}
/* below the content (#belowContent) */
/* HEADINGS & STATUS MESSAGES */
/* ordinary headings */
#content_box h3 {
    font-size: 1.09em; /* ~12px */
    text-align: left;
    margin: 20px 0 5px;
}
#content_box h3 span  a
{
    font-size: 10px;
}
#content h4 {
    font-size: 1.0em; /* ~11px */
    margin: 12px 0 8px;
}
#content h5 {
    font-size: 0.91em; /* ~10px */
    margin: 10px 0 7px;
}
#content h6 {
    font-size: 0.83em; /* 9px */
    margin: 9px 0 6px;
}
/* status headings */
h3#statusHeading {
    font-size: 1.27em; /* ~14px */
    line-height: 1.6em;
    height: 1.6em;
    background-color: #fff;
    margin: -10px -10px 0 -15px;
    padding: 0 5px 0 45px;
    background-repeat: no-repeat;
    background-position: 25px;
}
h3.critical {
    background: url(<%= asset_path 'icons/status/small/critical.gif' %>);
    border-top: 3px solid #9e0000;
    border-bottom: 3px solid #9e0000;
    color: #9e0000;
}
h3.information {
    background: url(<%= asset_path 'icons/status/small/info.gif' %>);
    border-top: 3px solid #0069a5;
    border-bottom: 3px solid #0069a5;
    color: #0069a5;
}
h3.ok {
    background: url(<%= asset_path 'icons/status/small/ok.gif' %>);
    border-top: 3px solid #6c8a28;
    border-bottom: 3px solid #6c8a28;
    color: #6c8a28;
}
h3.warning {
    background: url(<%= asset_path 'icons/status/small/warning.gif' %>);
    border-top: 3px solid #e8a507;
    border-bottom: 3px solid #e8a507;
    color: #e8a507;
}
h3.warning-message{
    color: #000;
    font-weight: normal;
}
h3.warning-message span{
    font-weight:bold; 
    background-color:#FC0;
    display: inline-block;
    padding: 0 5px;
}
#statusHeading span img {
    margin: 0 -3px 0 2px;
}
/* status message boxes */
#messages  {
    margin: -10px 10px 0px 10px;
}
#shadow-container {
    position: relative;
    left: 3px;
    top: 3px;
    margin-right: 23px;
    margin-bottom: 3px;
    margin-top: -6px;
}
#shadow-container .main-shadow2,
#shadow-container .main-shadow3,
#shadow-container .infospace-shadow2,
#shadow-container .infospace-shadow3,
#shadow-container .logon-shadow2,
#shadow-container .logon-shadow3,
#shadow-container .statusMessage {
    position: relative;
    left: -1px;
    top: -1px;
}
#shadow-container .main-shadow1 {
    background: #bdc6cb;
}
#shadow-container .main-shadow2 {
    background: #a6acaf;
}
#shadow-container .main-shadow3 {
    background: #8a8e90;
}
#shadow-container .infospace-shadow1 {
    background: #c0c1c1;
}
#shadow-container .infospace-shadow2 {
    background: #a0a1a1;
}
#shadow-container .infospace-shadow3 {
    background: #7f7f7f;
}
#shadow-container .logon-shadow1 {
    background: #f1f0f1;
}
#shadow-container .logon-shadow2 {
    background: #dbdadb;
}
#shadow-container .logon-shadow3 {
    background: #b8b6b8;
}
.statusMessage {
    margin: 15px 0px 15px 0px;
    font-size: 1.09em; /* ~12px */
}
.statusMessage p {
    font-size: 0.92em; /* ~11px */
    margin: 0;
    padding: 5px;
    background-color: #fff;
}
.statusMessage p.fixedHeightMessage {
    height: 13px;
    white-space: nowrap;
    overflow:hidden;
    text-overflow: ellipsis;
}
.statusMessage h6 {
    font-weight: bold;
    padding: 4px 0 4px 5px;
    margin: 0;
    color: #fff;
    font-size: 0.92em; /* ~11px */
}
.statusMessage div {
    background: #fff;
    color: #000;
}
.statusMessage#OK {
    border: 1px solid #6c8a28;
    background-color: #6c8a28;
}
.statusMessage#warning {
    border: 1px solid #eba507;
    background-color: #eba507;
}
.statusMessage#critical,
.statusMessage#error,
.statusMessage#alert {
    border: 1px solid #9e0000;
    background-color: #9e0000;
}
.statusMessage#notice,
.statusMessage#info {
    border: 1px solid #0069a5;
    background-color: #0069a5;
}
/* various headings */
#content h4.timeRange {
    text-align: center;
    margin: 0 0 10px;
    font-size: 1.09em; /* ~12px */
}
/* content-area tabs */
div.tab-container {
    margin: 0 -10px 0 -15px;
    padding: 15px 0 0;
}
ul.tabs {
    list-style-type: none;
    margin: -5px 0 -1px;
    padding: 0 15px;
    height: 30px;
    position: relative;
    top: 2px;
    z-index: 11;
    background: #ffffff url(<%= asset_path 'tabs/content_tabs_background.gif' %>) repeat-x bottom;
}
ul.tabs-standalone {
    margin: 0px -5px -1px -5px;
    padding: 19px 15px 0px 15px;
    top: 5px;
    background: #dee0e0 url(<%= asset_path 'tabs/content_tabs_background.gif' %>) repeat-x bottom;
}
ul.tabs-with-content-border {
    padding-bottom: 0px;
    margin-bottom: 0px;
    padding-left:0px
}
/* HACK: IE needs different values from the rest, so we hide this rule from it */
html>body ul.tabs {
    position: relative;
    top: 0;
}

.pageSection ul,ul.my_dashboard_tabs {
    font-size: 1.0em; /* ~11px */
    padding: 0 0 0 10px;
    height: 40px;
    margin: 0;
    position: relative;
    list-style-type: none;
    float: left;
   
}
.dashboard .pageSection{padding-left:5px;}
.pageSection ul li,ul.my_dashboard_tabs li {
    display:inline;
    visibility: visible;
    float: left;
    height: 25px;
    position: relative;
    top: 15px;
    text-align: center;
    border-right: 1px #ffffff;
    background: url(<%= asset_path 'tabs/content_inactive_tab_left.gif' %>) no-repeat top left;
    white-space: nowrap;
    border-top: 1px;
    border-top-color: #a8c3e1;
    padding-left:5px;
}
.pageSection ul li a:active,ul.my_dashboard_tabs li a:active {
    color: #004376;
}
.pageSection UL LI a:hover,ul.my_dashboard_tabs li a:hover {
    color: #437ba9;
}

.pageSection li.last a,ul.my_dashboard_tabs li.last a {
    border: none;
}
.pageSection li a,ul.my_dashboard_tabs li a {
    margin: 0 0 1px;
    float: left;
    display: block;
    padding: 6px 15px 0px 10px;
    text-align: center;
    text-decoration: none;
    color: #000000;
    height: 17px;
    display: block;
    background: url(<%= asset_path 'tabs/content_inactive_tab_right.gif' %>) no-repeat top right;
}
.pageSection li.selected,ul.my_dashboard_tabs li.current {
    visibility:visible;
    background: url(<%= asset_path 'tabs/content_active_tab_left.gif' %>)  no-repeat top left;
    text-align: center;
    padding-left:5px;
    margin: 0 0 0 0 ;
    position: relative;
    top: 10px;
    height: 30px;
    border-bottom: 1px solid #fff;
    font-weight: bold;
    z-index: 11;
}
.pageSection li.selected a,ul.my_dashboard_tabs li.current a  {
    margin: 0;
    font-weight: bold;
    color: #01577f;
    height: 22px;
    padding-top: 8px;
    background: url(<%= asset_path 'tabs/content_active_tab_right.gif' %>) no-repeat top right;
    font-size: 11px;
}
.pageSection li.selected a:link,ul.my_dashboard_tabs li.current a:link {
    color: #004376;
}
.pageSection li.selected a:visited ,ul.my_dashboard_tabs li.current a:visited{
    color: #004376;
}
.pageSection li.selected a:hover ,ul.my_dashboard_tabs li.current a:hover{
    color: #01577f;
}
/* content-area tree */
#mainDisplay div.tree {
    overflow: auto;
    width: 250px;
    height: 400px;
    border: 1px solid #dee0e0;
    padding: 5px;
    margin: 10px;
    background-color: #eff3f5;
}
/* TABLE STYLES */
table {
    margin: 10px 0px;
}
table caption {
    display: none;
}
table th {
    vertical-align: top;
    text-align: left;
    padding-right: 20px;
}
table ul, table ol {
    margin: 0 0 0 12px;
    padding: 0;
}
table td {
    padding-bottom: 10px;
}
table.data th.actives {
    background: #00418c url(<%= asset_path 'tables/header_active.gif' %>) repeat-x top;
}
table.data th a:hover {
    color: #d4dfe5;
}
table.data th#componentStates {
    width: 240px;
}
table.data tr.actions {
    background: #d4dfe5 url(<%= asset_path 'chart_menu_background.gif' %>) no-repeat top right;
}
table.data tr.actionsNoIconTray {
    background: #d4dfe5;
}
table.data tr.actions ul {
    float: right;
    list-style-type: none;
    margin: 0;
    padding: 0;
}
table.data tr.actions ul li {
    display: inline;
    padding: 0 4px;
}
table.data tr.actionsNoIconTray ul {
    float: right;
    list-style-type: none;
    margin: 0;
    padding: 0;
}
table.data tr.actionsNoIconTray ul li {
    display: inline;
    padding: 0 4px;
}
/* table row hover state */
table.data tr.hover {
    background-color: #c1e1f5 !important;
    border-top: 1px solid #b8cc69;
    border-bottom: 1px solid #b8cc69;
}
table.data tr.hover td {
}
/* table row selected state */
table.data tr.active {
    background-color: #86c6ec !important;
}
table.data tr.active td {
    border-right: 1px solid #86c6ec !important;
    border-bottom: 1px solid #b8cc69;
}
table.data tr.superHeader th {
    background: #5A8293;
    border: none;
}
table.data tr.superHeader th.superSpan {
    background: #066;
    text-align: center;
}
table.data tr.superHeader th.superSpan div.superSpanSide {
    background: url(<%= asset_path 'tables/data_table_superheader_span_right.gif' %>) right bottom no-repeat;
    width: 100%;
    margin-left: 1px;
    padding-bottom: 2px;
}	
table.data tr.superHeader th.superSpan span.title {
    background: #5A8293;
    padding: 0px 5px 0px 5px;
}
table.data td.center {
    text-align: center;
}
table.data tr.center td {
    text-align: center;
}
table.data td.image img {
    display: block;
    margin: auto;
}
table.data th.tableSectionHeader {
    background: #426f70;
    font-weight: bold;
    text-align: center;
    color: #fff;
    border: none;
}
table.data td {
    vertical-align: middle;
    padding: 3px 5px;
    border-right: 1px solid #ccc;
    border-bottom: 1px solid #dee0e0;
}
table.data tr.alternate {
    background-color: #eee;
}
table.data tr.filter td {
    background: #dee0e0;
    border-bottom: 1px solid #999;
    vertical-align: middle;
    padding: 6px 5px;
}
table.data tr.filter td ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
}
html>body table.data tr.filter td ul {
    position: relative;
    top: -3px;
}
table.data tr.filter td ul li {
    display: inline;
    padding-right: 5px;
}
html>body table.data tr.filter td ul li input.checkbox {
    position: relative;
    top: 3px;
}
table.data tr.filter div.search {
    float: right;
    margin: 0 10px 0 0;
}
/* pagination */
table.data div.nav {
    padding: 0 0 3px;
    position: relative;
    top: -5px;
}
table.data div.nav img {
    position: relative;
    top: 3px;
}
/* component status bar */	
table.componentStateTable {
    border: 1px solid #000;
    width: 220px;
    margin: 0 10px;
}
table.componentStateTable thead {
    display: none;
}
table.componentStateTable td {
    border: none;
    line-height: 1.0em;
    text-align: center;
    white-space: nowrap;
    height: 15px;
    border-right: 1px solid #000;
    background: #fff;
}
table.componentStateTable td img {
    display: inline;
    position: relative;
    top: 1px;
}
table.componentStateTable td.critical {
    background: url(<%= asset_path 'status_bar_background_critical.gif' %>) repeat-x top;
}
table.componentStateTable td.warning {
    background: url(<%= asset_path 'status_bar_background_warning.gif' %>) repeat-x top;
}
table.componentStateTable td.ok {
    background: url(<%= asset_path 'status_bar_background_ok.gif' %>) repeat-x top;
}
/* table for organizing form controls */
table.inputFields {
    margin: 0 0 15px 10px;
}
#aboveContent table.inputFields {
    margin-bottom: 0;
    margin-left: 0;
}
/* HACK: IE can exhibit really weird positioning bugs with some form 
		 elements, so let's be safe */
* html table.inputFields {
    x-position: relative;
}
table.inputFields tr {
    line-height: 1.4em;
}
table.inputFields td {
    padding: 3px 15px 3px 0;
    vertical-align: top;
}
table.inputFields th {
    padding: 4px 0 2px;
    font-weight: normal;
    text-align: left;
    vertical-align: top;
    padding-right: 10px;
}
#aboveContent table.inputFields th {
    text-align: right;
}
table.inputFields td img {
    position: relative;
    top: 2px;
}
table.inputFields td p {
    margin: 2px 0 6px;
}
table.inputFields input.checkbox {
    margin-right: 2px;
}

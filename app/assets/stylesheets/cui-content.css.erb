div#state_indicator {
	width: 340px;
	float: right;
    padding:1px 1px;
	text-align: center;
}
.inline{ display:inline!important;}
#state_indicator ul {
    list-style: none;
    list-style-position: outside;
    list-style-type: none;
	display: block;
    margin: 0;
    padding: 0;
    height: 20px;
}

#state_indicator li{ 
	text-align:center; 
	float: left; 
	width: 55px; 
	font-size: 11px; 
	border-right: 1px solid #ccc; 
	display: block; 
	margin: 0; 
	padding: 2px 2px;}
#state_indicator li.inactive{ color: #666; background-color: #e4e4e4; }
#state_indicator li.active{ width: 60px; color: #000; background-color: #C1E1F5; font-weight: bold; }
#state_indicator li.last{border-right: none;}
#state_indicator li.state_transition {background-color: #C1E1F5; width: 12px;}


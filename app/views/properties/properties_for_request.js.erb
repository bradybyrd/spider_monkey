if($('#argument_grid').length > 0) {
  $('#argument_grid').data('step_obj', JSON.parse("<%= j @step.to_json.html_safe %>"));
}

<% if params[:load_component_version].eql?("1") %>
  $("#<%= params[:container] %>").html("<%= j render :partial => 'steps/property_installed_components', :locals => { :request => @request, :step => @step,
    :installed_component => @step.installed_component, :work_task => @work_task } %>");
<% elsif params[:load_package_properties].eql?("1") %>
  $("#<%= params[:container] %>").html("<%= j render :partial => 'steps/property_packages', :locals => { :request => @request, :step => @step,
    :package => @step.package, :work_task => @work_task } %>");
<% elsif params[:load_package_instance_properties].eql?("1") %>
  $("#<%= params[:container] %>").html("<%= j render :partial => 'steps/property_package_instances', :locals => { :request => @request, :step => @step,
    :package_instance => @step.package_instance, :work_task => @work_task } %>");
<% else %>
  $("#<%= params[:container] %>").html("<%= j render :partial => 'steps/property_values', :locals => { :request => @request, :step => @step,
    :installed_component => @step.installed_component, :work_task => @work_task } %>")
<% end %>

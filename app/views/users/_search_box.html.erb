<% if can? :list, User.new %>
  <%= form_tag(users_path, :method => :get) do %>
    <%= text_field_tag :key, params[:key], :size => '15' %>
    <%= submit_tag 'Search',:class =>'button' %>
    <%= link_to "clear", "#{context_root}/users" %>
  <% end %>
<% end %>


<% draw_tabs selected: selected,
             only_ul: local_assigns[:only_ul] ? only_ul : nil,
             ul_class: local_assigns[:ul_class] ? ul_class : nil do %>
    <%= users_sub_tabs %>
    <% if can? :access, :settings %>
      <% if local_assigns[:tab_view] %>
        <%= sub_tab 'Settings' %>
      <% else %>
        <%= next_level_sub_tab 'Settings', { path: settings_path } %>
      <% end %>
    <% end %>
<% end %>

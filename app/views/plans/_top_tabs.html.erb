<% draw_tabs :selected => selected, :only_ul => local_assigns[:only_ul] ? only_ul : nil, :ul_class => local_assigns[:ul_class] ? ul_class : nil do %>
  <% if can? :list, Plan.new %>
    <%= sub_tab "Plans", :path => plans_path %>
  <% end %>

  <% if can? :list, Activity.new %>
    <%= sub_tab "Projects", :path => (session[:category_id] == "32" ? activity_category_path(session[:category_id]) : activities_path), :controller => 'activity_categories' %>
  <% end %>
<% end %>

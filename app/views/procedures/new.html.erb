<% backlink = procedures_path %>
<% title = "" %>
<% page_settings :title => title %>
<% content_for :sidebar do %>
  <!-- No content... -->
<% end %>
<% content_for :page_section do %>
  <%= render 'account/environment_tabs', :selected => 'metadata' %>
<% end %>
<% content_for :sidebar do %>
  <span></span>
<% end %>
<%= render :partial => 'environments/metadata_heading', :locals => {:backlink => backlink, :title => title} %>
<br/>
<br/>
<%= form_for @procedure, :url => :new_procedure_template_procedures, :html => { :class => 'cssform', :id => "new_procedure_template" } do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <div class='field'>
    <%= f.submit I18n.t(:create)%>
    or
    <%= link_to 'cancel', backlink %>
  </div>
<% end %>

<% unless external_script_output.blank? -%>
	<% if external_script_output[:data].try(:length) < 2 -%>
	<%= content_tag :h3, "Your query returned no matching tickets.  Adjust your filter parameters and retry?" %>
	<% else -%>
	<%= table_type_argument_body(1, external_script_output, nil, Step.new)  %>
	<%= mapped_table_type_argument(1, external_script_output) %>
	<%= hidden_field_tag(:project_server_id, project_server_id) unless project_server_id.blank? %>
	<%= submit_tag 'Assign Selected Tickets to Plan', :name => nil, :id => 'assign_tickets_to_plan_button', 'data-disable-with' => 'Assigning Tickets...', :class =>'button', :id => "tickets_assign_to_plan"  %>
	<% end -%>
<% end -%>

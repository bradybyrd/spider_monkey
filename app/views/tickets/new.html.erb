<% backlink = tickets_path %>
<% title = 'Create New Ticket' %>
<% page_settings :title => title %>
<% content_for :head do %>
  <%= static_javascript_include_tag 'tickets' %>
<% end %>
<% content_for :page_section do %>
  <%= render 'account/environment_tabs', :selected => 'metadata' %>
<% end %>
<% content_for :sidebar do %>
  <span></span>
<% end %>
<%= render :partial => 'environments/metadata_heading', :locals => {:backlink => backlink, :title => title} %>
<br class='clear'/>
<br/>
<br/>
<div id="ticket_form_container">
  <%= render :partial => 'form', :locals => { :ticket => @ticket, :backlink => tickets_path } %>
</div>  

<%= f.error_messages %>
<div class='field'>
  <%= f.label :name %>
  <%= f.text_field :name, :class => 'initialFocus' %>
</div>
<% if not f.object.new_record? %>
  <div class='field'>
    <%= f.label 'Instance Name Format' %>
    <%= f.text_field :instance_name_format %>
  </div>
  <div class='field'>
    <%= f.label 'Next Instance Number' %>
    <%= f.text_field :next_instance_number %>
  </div>
<% end %>
<%= hidden_field_tag 'page', params[:page] %>
<%= hidden_field_tag 'key', params[:key] %>

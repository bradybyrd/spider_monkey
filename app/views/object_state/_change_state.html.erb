<%
  unless state_object.state.blank?
    state_object.class.events_hash.each do |event_name, event_human_name|
      if state_object.send("may_#{event_name}?")
%>
        <%= link_to event_human_name, eval("#{event_name}_deployment_window_path(state_object)"), :method => :put, :class => 'button' %><br/>
<%
      end
    end
  end
%>



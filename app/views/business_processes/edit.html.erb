<% backlink = processes_path %>
<% title = "Process: #{h(@business_process.name)}" %>
<% page_settings :title => title %>
<% content_for :page_section do %>
  <%= render 'account/environment_tabs', :selected => 'metadata' %>
<% end %>
<% content_for :sidebar do %>
  <span></span>
<% end %>
<%= render :partial => 'environments/metadata_heading', :locals => {:backlink => backlink, :title => title} %>
<br>
<br>
<%= form_for @business_process, :html => { :class => 'cssform' } do |f| %>
  <%= render 'form', :f => f %>
  <div class='field'>
    <%= f.submit "Update", :class => "button" %>
    or
    <%= link_to 'cancel', backlink %>
  </div>
<% end %>


<% backlink = deployment_window_series_index_path %>
<% title = "Create #{@deployment_window_series.behavior.capitalize} Deployment Window Series" %>
<% page_settings :title => title %>
<% content_for :page_section do %>
    <%= render 'account/environment_tabs', :selected => 'metadata' %>
<% end %>
<% content_for :sidebar do %>
    <span></span>
<% end %>
<%= render :partial => 'environments/metadata_heading', :locals => {:backlink => backlink, :title => title} %>
<br class='clear'/>
<%= form_for @deployment_window_series, html: { class: 'cssform' } do |f| %>
    <%= render 'form', :f => f %>
    <div class='field'>
      <%= f.submit I18n.t(:create), class: "button dws_form_submit" %>
      or
      <%= link_to 'cancel', backlink %>
    </div>
<% end %>
